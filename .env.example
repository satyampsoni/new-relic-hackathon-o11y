# New Relic Configuration
# Copy this file to .env and fill in your actual values

# Your New Relic Ingest License Key
# Get this from: New Relic → Account Settings → API Keys → Create key (Ingest - License type)
NEW_RELIC_API_KEY=your_ingest_license_key_here

# Your New Relic Account ID
# Find this in your New Relic URL or Account Settings
NEW_RELIC_ACCOUNT_ID=your_account_id_here

# Optional: Override default configuration
# LOG_LEVEL=info
# PORT=8080
# WORKER_COUNT=4

# Optional: Override New Relic region (US or EU)
# NEW_RELIC_REGION=US
